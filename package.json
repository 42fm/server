{
  "name": "twitch-music-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node --import @bleed-believer/path-alias --env-file=.env --loader=ts-node/esm --watch src/index.ts",
    "dev:docker": "node --import @bleed-believer/path-alias --loader=ts-node/esm --watch src/index.ts",
    "start": "node ./dist/index.js",
    "build": "rm -rf dist/ && tsc -p . && tsc-alias",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migration:generate": "typeorm-ts-node-esm migration:generate --pretty -d src/db/index.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/db/index.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/db/index.ts"
  },
  "dependencies": {
    "@bleed-believer/path-alias": "^1.1.2",
    "@googleapis/youtube": "^16.0.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.6.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "iso8601-duration": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.7.5",
    "tmi.js": "^1.8.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.2.2",
    "winston": "^3.13.0",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/tmi.js": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.2.0",
    "globals": "^15.1.0",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^7.8.0"
  },
  "imports": {
    "#tmi": {
      "node": "tmi.js",
      "default": "./node_modules/tmi.js/index.js"
    }
  }
}
