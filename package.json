{
  "name": "twitch-music-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev -r dotenv/config -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "dev:docker": "ts-node-dev -r dotenv/config -r tsconfig-paths/register --poll --respawn --transpile-only src/index.ts",
    "start": "node --env-file=.env ./dist/index.js",
    "build": "rm -rf dist/ && tsc -p . && tsc-alias",
    "lint": "eslint src/**/*.ts ./**/*.ts",
    "lint:fix": "eslint src/**/*.ts ./**/*.ts --fix",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d src/db/index.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/db/index.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/db/index.ts"
  },
  "dependencies": {
    "@googleapis/youtube": "^9.0.0",
    "@socket.io/admin-ui": "^0.4.0",
    "@socket.io/redis-adapter": "^7.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/tmi.js": "^1.8.1",
    "axios": "^1.6.8",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "ioredis": "^5.3.2",
    "iso8601-duration": "^2.1.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "tmi.js": "^1.8.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.16",
    "typescript": "^5.2.2",
    "winston": "^3.5.1",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0"
  }
}
