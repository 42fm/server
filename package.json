{
  "name": "twitch-music-server",
  "version": "1.0.0",
  "main": "src/index.ts",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "ts-node-esm ./dist/src/index.js",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d src/db/index.ts",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/db/index.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/db/index.ts"
  },
  "dependencies": {
    "@socket.io/admin-ui": "^0.4.0",
    "@socket.io/redis-adapter": "^7.1.0",
    "@types/ioredis": "^4.28.7",
    "axios": "^0.27.2",
    "bullmq": "^1.66.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.17.2",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-oauth": "^1.0.0",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "tmi.js": "^1.8.5",
    "typeorm": "^0.3.7",
    "winston": "^3.5.1",
    "ytdl-core": "4.10.0",
    "ytsr": "^3.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/tmi.js": "^1.8.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.5"
  }
}
