{
  "name": "twitch-music-server",
  "version": "1.0.0",
  "main": "dist/index.js",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "common": "node --no-warnings --enable-source-maps --experimental-test-module-mocks --import @swc-node/register/esm-register",
    "dev": "yarn common --watch --env-file=.env src/index.ts",
    "dev:docker": "yarn common --watch src/index.ts",
    "start": "node ./dist/index.js",
    "build": "yarn typecheck && yarn clean && swc src -d dist --strip-leading-paths",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "yarn common --test test/**/*.test.ts",
    "test:dev": "yarn common --test test/**/*.test.ts",
    "migration:generate": "yarn build && typeorm-ts-node-esm migration:generate --pretty -d src/db/index.ts",
    "migration:create": "yarn build && typeorm-ts-node-esm migration:create",
    "migration:run": "yarn build && typeorm-ts-node-esm migration:run -d src/db/index.ts",
    "migration:revert": "yarn build && typeorm-ts-node-esm migration:revert -d src/db/index.ts"
  },
  "dependencies": {
    "@bleed-believer/path-alias": "^2.2.1",
    "@googleapis/youtube": "^16.1.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/tmi.js": "^1.8.6",
    "axios": "^1.8.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "ioredis": "^5.6.0",
    "iso8601-duration": "^2.1.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "tmi.js": "^1.8.5",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.21",
    "typescript": "^5.8.2",
    "winston": "^3.17.0",
    "ytdl-core": "^4.11.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.7",
    "@types/ioredis-mock": "^8.2.5",
    "@types/node": "^22.13.9",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "globals": "^15.15.0",
    "ioredis-mock": "^8.9.0",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.26.0",
    "why-is-node-running": "^3.2.2",
    "ws": "^8.18.1"
  },
  "imports": {
    "#tmi": {
      "node": "tmi.js",
      "default": "./node_modules/tmi.js/index.js"
    }
  },
  "packageManager": "yarn@4.6.0"
}
